
entity Visit {
	startDate Instant,
	endDate Instant,
}

entity Pet {
	name String,
    type PetType,
}

enum PetType {
    Dog, Cat
}

entity Owner {
	name String,
    address String,
    phone String
}

entity Vet {
	name String,
    phone String
}

relationship OneToMany {
	Owner{pets} to Pet{owner},
	Pet{visits} to Visit{pet},
	Vet{visits} to Visit{vet}
}

service all with serviceImpl
